<launch>

    <param name="use_sim_time" value="true"/>
    <arg name="fssim_config" default="$(find fssim)/config/simulation.yaml"/>
    <arg name="topics_conversion_config" default="$(find fssim)/config/topics_conversion.yaml"/>
    <include file="$(find fssim)/launch/start.launch">
        <arg name="fssim_config" value="$(arg fssim_config)"/>
    </include>

    <include file="$(find fssim)/launch/rviz.launch"/>

    <include file="$(find fssim)/launch/msgs_convert.launch">
        <arg name="topics_conversion_config" value="$(arg topics_conversion_config)"/>
    </include>
</launch>
